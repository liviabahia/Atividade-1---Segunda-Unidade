package clinicamedica;
public class Paciente {
  private int codigo; 
  private String nome, dataNascimento, sexo, planoSaude, alergia, tipoSanguineo;
  public Paciente(int codigo, String nome, String dataNasc, String sexo, String planoS, String alergia, String tipoS){
    this.codigo = codigo;
    this.nome = nome;
    this.dataNascimento = dataNasc;
    this.sexo = sexo;
    this.planoSaude = planoS;
    this.alergia = alergia;
    this.tipoSanguineo = tipoS;
}
     public int getCodigo(){
        return this.codigo; 
        }
         public String getNome (){ 
           return this.nome; 
           } 
           public String getDataNascimento (){
              return this.dataNascimento; 
              }
               public String getSexo (){ 
                 return this.sexo; 
                 } 
                 public String getPlanoSaude (){ 
                   return this.planoSaude; 
                   }
                    public String getAlergia (){ 
                     return this.alergia; 
                     }
                      public String getTipoSanguineo (){ 
                        return this.tipoSanguineo; 
                        } 
                         public void setCodigo(int cod){
                            this.codigo = codigo; 
                            } 
                            public void setNome (String nome) { 
                              this.nome = nome; 
                              }
                               public void setDataNascimento (String data) {
                                  this.dataNascimento = dataNasc; 
                                  } 
                                   public void setSexo (String sexo) { 
                                     this.sexo = sexo; 
                                     }
                                      public void setAlergia (String alergia) {
                                         this.alergia = alergia; 
                                         } 
                                         public void setTipoSanguineo (String sgl) {
                                            this.tipoSanguineo = tipoS ;
                                             }
                                              public void setPlanoSaude(String plano){
                                                this.planoSaude = planoS; 
                                                }
                                                 }
    
