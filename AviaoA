public class AviaoA {
   public static void main(String[] args){
   Aviao executiva = new Aviao(900, 10);
   System.out.println("\nVelocidade: " + executiva.getVelocidade() + " Km/h");
   System.out.println("Altitude: " + executiva.getAltitude() + " Km\n");
   
   executiva.diminuirAltitude(10); 
   executiva.diminuirVelocidade(100);
   
   System.out.println("\nVelocidade: " + executiva.getVelocidade() + " Km/h");
   System.out.println("Altitude: "+ executiva.getAltitude() + " Km\n"); 
   }
   }
