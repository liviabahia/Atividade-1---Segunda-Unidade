public class Postp {
  
   public static void main(String args[])
   { 
     post w = new post("Café com leite ♡", "https://youtu.be/kjIgcgqqh38"); 
    w.curtir();
    w.curtir();
    w.curtir();
    
    System.out.println("\nCurtidas:" +w.getNumCurtidas());
    
    w.compartilhar();
    w.compartilhar();
    
    System.out.println("\nCompartilhamentos:" +w.getNumComp());
    
   }
 }
